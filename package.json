{
  "name": "blog-server-ts",
  "version": "1.0.1",
  "description": "blog server base on koa2+typescript",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch ./src/**/* -e ts,tsx --exec ts-node ./src/app.ts",
    "start": "set NODE_ENV=production&&pm2 start ./dist/app.js",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "mysql": "ts-node ./src/db/index.ts",
    "build-ts": "tsc",
    "build": "npm run tslint && npm run build-ts"
  },
  "author": "chance722",
  "license": "ISC",
  "dependencies": {
    "busboy": "^0.2.14",
    "debug": "^2.6.3",
    "geoip-lite": "^1.3.5",
    "js-base64": "^2.4.3",
    "jsonwebtoken": "^8.2.0",
    "koa": "^2.6.2",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^3.5.1",
    "koa-logger": "^2.0.1",
    "koa-router": "^7.4.0",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.1",
    "koa2-cors": "^2.0.5",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.2",
    "qiniu": "^7.1.3",
    "qn": "^1.3.0",
    "querystring": "^0.2.0",
    "request": "^2.83.0",
    "sequelize": "^4.34.0",
    "simple-netease-cloud-music": "^0.4.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^7.2.6",
    "@types/node": "^10.12.12",
    "@types/koa": "^2.0.47",
    "@types/koa-router": "^7.0.35",
    "nodemon": "^1.18.7",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.1"
  }
}
